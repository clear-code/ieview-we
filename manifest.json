{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "2.0.1",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "contextMenus",
    "nativeMessaging",
    "storage",
    "alarms",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "icons": {
    "128": "misc/128x128.png"
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "default_locale": "en",
  "applications": {
    "gecko": {
      "id": "ieview-we@clear-code.com",
      "strict_min_version": "52.0"
    }
  }
}
